  "keywords": ["laravel", "lumen", "umeng"],
  "homepage": "https://github.com/zhaolong06/laravel-umeng",
  "license": "MIT",
  "authors": [
    {
      "name": "zhengzhaolong",
      "email": "zhaolong0206@gmail.com"
    }
